      <%= link_to t('.maintenance'), profile_maintenance_path(:id => @user.id), :class => "tablink user-maintenance-tab", :remote => true %>
    </div>
  </div>
</div>
<div id="render-profile-view">
  <%= render :template => 'users/profile_maintenance.html' %>
</div>