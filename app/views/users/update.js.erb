<% if @user.errors.any? %>
	$('.head-user').html("<%= j (render :partial => 'users/form_header') %>");
	user_update_js();
	$('.dropify-render').html("<%= j (image_tag @user.photo(:normal)) %>")
	$('.dropify-preview').css('display', 'block')
<% else %>
	$('.head-user').html("<%= j (render :partial => 'users/header_user') %>");
<% end %>